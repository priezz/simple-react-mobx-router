{
  "name": "simple-react-mobx-router",
  "version": "1.0.1",
  "README": "README.md",
  "author": "Alexandr Priezzhev",
  "description": "Simple router for React projects based on MobX reactivity",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/priezz/simple-react-mobx-router"
  },
  "scripts": {
    "build": "npm install && npm update && babel --stage 0 --optional runtime src --out-dir .",
    "deploy": "npm run build && git add . && git commit && git push && npm publish"
  },
  "dependencies": {
    "mobx": "^3.1.5",
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^3.17.1"
  },
  "babel": {
    "presets": [
      "react-native-stage-0/decorator-support"
    ],
    "passPerPreset": false,
    "plugins": [
      "syntax-flow",
      "transform-flow-strip-types"
    ]
  }
}
